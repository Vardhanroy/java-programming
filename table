import java.util.*;
public class table{
public static void main(String[] args){
try
{
Scanner input=new Scanner(System.in);
System.out.println("enter the multiplier:");
int n=input.nextInt();
for(int i=1;i<=10;i++)
{
System.out.println(n+"*"+i+"="+(i*n));
}
}
catch(Exception e)
{
System.out.println("enter the valid number");
}
}
}
